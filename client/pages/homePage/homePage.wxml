<!--pages/homePage/homePage.wxml-->
<view class='avatar'>
    <image src='{{avatarUrl}}' class='avatar-image'></image>
</view>
<view class='nickname'>
    <text>{{nickname}}</text>
</view>

<view class='icon-wrap'>
    <view wx:for="{{icons}}" wx:for-item="icon" class='icon' >
        <image  src="{{icon.imageUrl}} " class="icon-image"></image>
        <text class='icon-num'>{{icon.name}}:{{icon.num}}</text>
    </view>
</view>

<view class='ui-cells hr'>
    <view wx:for="{{linksList}}" wx:for-item="link"  class='ui-cell' >
        <view class='ui-cell-body'>{{link.name}}</view>
        <view class='ui-cell-foot-in-access'></view>
    </view>
</view>